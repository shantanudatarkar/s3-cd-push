pipeline {
    agent any

    stages {
        stage('Deploy to Prod') {
            environment {
                S3_BUCKET_NAME = "prod-shan-infra" // Replace with your prod S3 bucket name
            }
            steps {
                script {
                    // Add deployment steps for the prod environment
                    sh "aws s3 cp index.html s3://${S3_BUCKET_NAME}/index.html"
                }
            }
        }
    }
}